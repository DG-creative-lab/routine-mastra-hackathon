{
  "name": "routine-mastra-hackathon",
  "version": "0.1.0",
  "description": "Turns high-level marketing specs ➜ Routine plans ➜ Mastra templates.",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "meta": "ts-node src/meta-planner/planner.ts",
    "parse": "ts-node src/parser/index.ts src/parser/agents_spec.json",
    "template": "ts-node src/template-builder/run.ts",
    "mastra:dev": "mastra dev ./generated-templates",
    "mastra:deploy": "mastra deploy ./generated-templates",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@google-analytics/data": "^5.2.0",
    "@google-cloud/bigquery": "^8.1.1",
    "@googleapis/displayvideo": "^32.3.0",
    "@mastra/core": "^0.13.1",
    "archiver": "^7.0.1",
    "dotenv": "^17.2.1",
    "google-ads-api": "^20.0.1",
    "google-auth-library": "^10.2.1",
    "lucide-react": "^0.539.0",
    "mastra": "^0.10.20",
    "next": "^15.4.6",
    "openai": "^5.12.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/archiver": "^6.0.3",
    "@types/node": "^20.11.19",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "mastra"
      ]
    }
  }
}