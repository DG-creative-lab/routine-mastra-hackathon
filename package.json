    "google-auth-library": "^10.2.1",
    "lucide-react": "^0.539.0",
    "mastra": "^0.10.20",
    "next": "^15.4.6",
    "next-themes": "^0.4.6",
    "openai": "^5.12.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.23.8"
